PROG = PKImplode
DEF_FILE = ../Codec.def

# IS_X64 = 1
# USE_ASM = 1
# ST_MODE = 1

ifdef SystemDrive

LOCAL_FLAGS_WIN = \
  -D_7ZIP_LARGE_PAGES \
  $(LOCAL_FLAGS_ST) \

SYS_OBJS = \

else

SYS_OBJS = \
  $O/MyWindows.o \

endif

LOCAL_FLAGS = \
  -DEXTERNAL_CODECS \
  $(LOCAL_FLAGS_WIN) \
  $(LOCAL_FLAGS_ST) \

COMMON_OBJS = \
  $O/InBuffer.o \
  $O/OutBuffer.o \
  $O/StreamUtils.o \

COMPRESS_OBJS_2 = \
  $O/CodecExports.o \
  $O/DllExportsCompress.o \
  $O/Alloc.o \
  $O/PKImplodeRegister.o \
  $O/PKImplodeEncoder.o \
  $O/PKImplodeDecoder.o \
  $O/explode.o \
  $O/implode.o \

OBJS = \
  $(COMPRESS_OBJS_2) \
  $(SYS_OBJS) \
  $(COMMON_OBJS) \

include ../../7zip_gcc.mak

$O/PKImplodeRegister.o: Compress/PKImplodeRegister.cpp
	$(CXX) $(CXXFLAGS) $<
$O/PKImplodeEncoder.o: Compress/PKImplodeEncoder.cpp
	$(CXX) $(CXXFLAGS) $<
$O/PKImplodeDecoder.o: Compress/PKImplodeDecoder.cpp
	$(CXX) $(CXXFLAGS) $<
$O/explode.o: pklib/explode.c
	$(CC) $(CFLAGS) $<
$O/implode.o: pklib/implode.c
	$(CC) $(CFLAGS) $<
